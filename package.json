{
  "name": "logweb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf build && shx mkdir build",
    "jshint": "node_modules/.bin/jshint src",
    "pretest": "npm run jshint",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register \"src/**/*.spec.js\"",
    "build:dist": "node_modules/.bin/babel src -d build --source-maps --ignore spec.js",
    "build:dev": "node_modules/.bin/babel -w src -d build --source-maps --ignore spec.js",
    "dist": "npm run clean && npm run test && npm run build:dist",
    "start": "npm run build:dist && node build/xindex.js"
  },
  "author": "Pawel Chudzik",
  "license": "MIT",
  "homepage": "https://github.com/pchudzik/logweb",
  "repository": {
    "type": "git",
    "url": "git://github.com:pchudzik/logweb.git"
  },
  "bugs": {
    "url": "https://github.com/pchudzik/logweb/issues"
  },
  "dependencies": {
    "CBuffer": "2.0.0",
    "express": "4.14.0",
    "lodash": "4.15.0",
    "respawn": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "ws": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babelify": "7.3.0",
    "chai": "3.5.0",
    "jshint": "2.9.3",
    "mocha": "3.0.2",
    "nodemon": "1.10.2",
    "proxyquire": "1.7.10",
    "shx": "0.1.4",
    "supertest": "2.0.0",
    "testdouble": "1.6.1"
  }
}
