{
	"port": 8008,
	"defaultShell": [
		"sh",
		"-c"
	],
	"inputs": [
		{
			"name": "syslog",
			"cmd": "tail -f /var/log/syslog",
			"buffer": "5000"
		},
		{
			"name": "example",
			"providers": [
				{
					"name": "node2",
					"cmd": "node fake-input.js"
				}
			]
		},
		{
			"name": "two-nodes",
			"providers": [
				{
					"name": "node1",
					"cmd": [
						"./random.sh",
						"node1 logger"
					]
				},
				{
					"name": "node2",
					"cmd": [
						"./random.sh",
						"node2 logger"
					]
				}
			]
		}
	]
}
